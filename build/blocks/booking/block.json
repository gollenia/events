{
  "name": "events-manager/booking",
  "apiVersion": 3,
  "title": "Event Booking",
  "description": "Add a button that allows the user to book tickets for an event.",
  "category": "widgets",
  "attributes": {
    "buttonTitle": {
      "type": "string",
      "default": ""
    },
    "buttonColor": {
      "type": "string",
      "default": "primary"
    },
    "buttonIcon": {
      "type": "string",
      "default": ""
    },
    "iconRight": {
      "type": "boolean",
      "default": false
    }
  },
  "usesContext": [
    "postType"
  ],
  "supports": {
    "anchor": true,
    "html": false,
    "color": {
      "link": true,
      "text": true,
      "background": true
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": false,
      "width": false,
      "__experimentalDefaultControls": {
        "radius": true,
        "style": false,
        "width": true,
        "linked": false,
        "color": true
      }
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "styles": [
    {
      "name": "fill",
      "label": "Fill",
      "isDefault": true
    },
    {
      "name": "outline",
      "label": "Outline"
    }
  ],
  "editorStyle": "events-block-editor-style",
  "style": "booking-style",
  "render": "file:./render.php",
  "viewScript": "booking-view"
}