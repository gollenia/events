{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "events-manager/details",
	"apiVersion": 2,
	"title": "Event Details",
	"keywords": [ "list", "table" ],
	"description": "Show a list with selected details of the event",
	"textdomain": "events",
	"category": "widgets",
	"attributes": {
		"dividers": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"__experimentalOnEnter": true,
		"__experimentalSettings": true,
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true,
			"__experimentalDefaultControls": {
				"blockGap": true
			}
		},
		"__experimentalLayout": {
			"allowSizingOnChildren": true
		}
	},
	"editorStyle": "wp-block-descriptiopn-list-editor",
	"style": "wp-block-descriptiopn-list"
}
