{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "events-manager/details",
	"apiVersion": 3,
	"title": "Event Details",
	"keywords": [ "list", "table" ],
	"description": "Show a list with selected details of the event",
	"textdomain": "events",
	"category": "widgets",
	"attributes": {
		"dividers": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"anchor": true,
		"align": [ "wide", "full" ],
		"html": false,
		"layout": {
			"allowSwitching": false,
			"allowInheriting": false,
			"allowEditing": false,
			"default": {
				"type": "flex",
				"orientation": "vertical",
				"flexWrap": "wrap"
			}
		},
		"__experimantalLayout": {
			"default": {
				"default": {
					"type": "flex",
					"orientation": "vertical",
					"flexWrap": "wrap"
				}
			}
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": {
				"__experimentalDefault": "2em",
				"sides": [ "horizontal" ]
			},
			"__experimentalDefaultControls": {
				"padding": true,
				"blockGap": true
			}
		}
	},

	"editorStyle": "events-block-editor-style",
	"style": "events-block-style"
}
