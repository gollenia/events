{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "events-manager/details-item",
	"apiVersion": 2,
	"title": "Event Details Item",
	"keywords": [ "list", "table" ],
	"description": "Other information for an event",
	"textdomain": "events",
	"category": "widgets",
	"attributes": {
		"image": {
			"type": "object",
			"default": null
		},
		"title": {
			"type": "string",
			"default": "<b></b>"
		},
		"icon": {
			"type": "string",
			"default": ""
		},
		"text": {
			"type": "string",
			"default": ""
		},
		"url": {
			"type": "string",
			"default": ""
		},
		"urlIcon": {
			"type": "string",
			"default": "more_horiz"
		},
		"contentType": {
			"type": "string",
			"default": ""
		},
		"roundImage": {
			"type": "boolean",
			"default": false
		},
		"seperationLines": {
			"type": "boolean",
			"default": false
		},
		"iconColor": {
			"type": "string",
			"default": ""
		},
		"customIconColor": {
			"type": "string",
			"default": ""
		},
		"iconBackgroundColor": {
			"type": "string",
			"default": ""
		},
		"customIconBackgroundColor": {
			"type": "string",
			"default": ""
		}
	},
	"parent": [ "ctx-blocks/description-list" ],
	"supports": {
		"spacing": {
			"margin": [ "top", "bottom" ],
			"__experimentalDefaultControls": {
				"margin": true
			}
		}
	}
}
