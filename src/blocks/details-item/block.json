{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "events-manager/details-item",
	"apiVersion": 3,
	"title": "Other Item",
	"keywords": [ "list", "table" ],
	"description": "Create your very own description item with icon, image and custom content.",
	"textdomain": "events",
	"category": "common",
	"attributes": {
		"imageUrl": {
			"type": "string",
			"default": ""
		},
		"imageAlt": {
			"type": "string",
			"default": ""
		},
		"imageId": {
			"type": "number",
			"default": 0
		},
		"icon": {
			"type": "string",
			"default": "label"
		},
		"styleVariation": {
			"type": "string",
			"default": "icon"
		},
		"url": {
			"type": "string",
			"default": ""
		},
		"urlNewTab": {
			"type": "boolean",
			"default": false
		},
		"urlIcon": {
			"type": "string",
			"default": "more_horiz"
		},
		"roundImage": {
			"type": "boolean",
			"default": false
		},
		"seperationLines": {
			"type": "boolean",
			"default": false
		},
		"iconColor": {
			"type": "string",
			"default": ""
		},
		"customIconColor": {
			"type": "string",
			"default": ""
		},
		"iconBackgroundColor": {
			"type": "string",
			"default": ""
		},
		"customIconBackgroundColor": {
			"type": "string",
			"default": ""
		}
	},
	"parent": [ "events-manager/details" ],
	"supports": {
		"spacing": {
			"margin": [ "top", "bottom" ],
			"__experimentalDefaultControls": {
				"margin": true
			}
		},
		"color": {
			"background": true,
			"text": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		}
	},
	"selectors": {
		"color": ".wp-block-ctx-blocks-description-item .ctx__description-item-image",
		"border": ".wp-block-ctx-blocks-description-item .ctx__description-item-image"
	},
	"editorStyle": "events-block-editor-style",
	"style": "events-block-style"
}
